{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "target": { "limit": 100, "matchAny": false, "tags": [], "type": "dashboard" },
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": 28,
  "links": [],
  "liveNow": false,

  "panels": [
    {
      "id": 10,
      "type": "gauge",
      "title": "Active Sessions",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 10, "x": 0, "y": 0 },
      "targets": [
        { "refId": "A", "expr": "sms_checker_active_sessions", "range": true }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": [] },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },

    {
      "id": 5,
      "type": "timeseries",
      "title": "Latency (p50 / p95 / p99)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 10, "x": 10, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum by (le) (rate(sms_checker_classify_latency_seconds_bucket[5m])))",
          "legendFormat": "p50",
          "range": true
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(sms_checker_classify_latency_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "range": true
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(sms_checker_classify_latency_seconds_bucket[5m])))",
          "legendFormat": "p99",
          "range": true
        }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": [] },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },

    {
      "id": 4,
      "type": "heatmap",
      "title": "Classify Latency Distribution ($model_version)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 10, "w": 20, "x": 0, "y": 8 },
      "repeat": "model_version",
      "repeatDirection": "h",
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum by (le) (rate(sms_checker_classify_latency_seconds_bucket{model_version=~\"$model_version\"}[5m]))",
          "legendFormat": "{{le}}",
          "range": true
        }
      ],
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "show": true, "yHistogram": true }
      }
    },

    {
      "id": 8,
      "type": "gauge",
      "title": "In-flight request",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 10, "x": 0, "y": 18 },
      "targets": [
        { "refId": "A", "expr": "sms_checker_in_flight_requests", "range": true }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": [] },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },

    {
      "id": 2,
      "type": "timeseries",
      "title": "Actions total",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 10, "x": 10, "y": 18 },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(sms_checker_actions_total{action=\"classify\"}[5m])",
          "legendFormat": "__auto",
          "range": true
        }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": [] },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    }
  ],

  "schemaVersion": 37,
  "style": "dark",
  "tags": [],

  "templating": {
    "list": [
      {
        "name": "model_version",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(sms_checker_classify_latency_seconds_bucket, model_version)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },

  "time": { "from": "now-6h", "to": "now" },
